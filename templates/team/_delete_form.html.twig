<form method="post" action="{{ path('app_team_delete', {'id': team.id}) }}"
      onsubmit="return confirm('Confirmez-vous la suppression de cette équipe ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ team.id) }}">
    <button class="btn btn-danger">Supprimer cette équipe</button>
</form>


